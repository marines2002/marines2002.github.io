	<connectionStrings>
    
    <remove name="LocalSqlServer"/>
    <add name="LocalSqlServer" connectionString="Data Source=localhost;Initial Catalog=twotwentysolutions;Persist Security Info=True;User ID=nigelcharlton;Password=Marines01"/>
    <add name="ClubSiteDB" connectionString="Data Source=localhost;Initial Catalog=twotwentysolutions;Persist Security Info=True;User ID=nigelcharlton;Password=Marines01"/>

    <!--<add name="LocalSqlServer" connectionString="Data Source=localhost;Initial Catalog=twotwentysolutions;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="ClubSiteDB" connectionString="Server=NIGEL-PC;Initial Catalog=twotwentysolutions;Trusted_Connection=True"/>-->

   
